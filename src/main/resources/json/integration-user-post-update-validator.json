{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "definitions": {},
  "id": "integration-user-post-update-validator",
  "properties": {
    "userId": {
      "type": "string",
      "pattern": "[a-f0-9]{8}(?:-[a-f0-9]{4}){4}[a-f0-9]{8}",
      "maxLength": 36
    },
    "userName": {
      "type": "string",
      "minLength": 4,
      "maxLength": 150
    },
    "firstName": {
      "type": "string",
      "minLength": 4,
      "maxLength": 150
    },
    "lastName": {
      "type": "string",
      "minLength": 4,
      "maxLength": 150
    },
    "email": {
      "type": "string",
      "minLength": 4,
      "maxLength": 150
    },
    "realmName": {
      "type": "string",
      "minLength": 3,
      "maxLength": 150
    },
    "role": {
      "type": "string",
      "minLength": 4,
      "maxLength": 150
    },
    "group": {
      "type": "string",
      "minLength": 4,
      "maxLength": 150
    },
    "enabled": {
      "type": "boolean",
      "default": true
    }
  },
  "required": [
    "userId",
    "realmName"
  ],
  "type": "object"
}