{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "definitions": {},
  "id": "integration-user-post-update-password-validator",
  "type": "object",
  "properties": {
    "realmName": {
      "default": "Profile Name",
      "type": "string",
      "minLength": 4,
      "maxLength": 150
    },
    "userId": {
      "type": "string",
      "pattern": "[a-f0-9]{8}(?:-[a-f0-9]{4}){4}[a-f0-9]{8}",
      "maxLength": 36
    },
    "password": {
      "type": "string",
      "minLength": 4,
      "maxLength": 150
    }
  },
  "required": [
    "realmName",
    "userId",
    "password"
  ]
}